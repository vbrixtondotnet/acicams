
<div class="modal fade" id="mdlPolicy" tabindex="-1" role="basic" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">New Policy</h4>
            </div>

            <form class="form-horizontal" role="form" id="frmPolicy">
                <div class="modal-body" style="padding-bottom:0; padding-top:0;">
                    <div class="portlet light bordered">
                        <div class="portlet-body form policy-form">
                            <div class="row">
                                <div class="form-body" style="padding-top:0; padding-bottom:0">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Account</label>
                                            <div class="col-md-9">
                                                <select class="form-control" disabled id="slcPolicyAccountName"  data-model="accountId">
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Carrier</label>
                                            <div class="col-md-9">
                                                <select id="slcCarriers" class="form-control select-search" data-model="carrierId">
                                                    <option></option>
                                                </select>
                                                </div>
                                            </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">MGA</label>
                                            <div class="col-md-9">
                                                <select class="form-control" id="slcMgas" data-model="mgaid">
                                                    <option></option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Policy Number</label>
                                            <div class="col-md-9">
                                               <input type="text" class="form-control" data-model="policyNumber"/>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Coverage</label>
                                            <div class="col-md-9">
                                                <select class="form-control" id="slcPolicyCoverageType" data-model="coverageTypeId" required>
                                                    <option value=""></option>
                                                    <option value="1">Auto Liability</option>
                                                    <option value="2">Motor Truck Cargo</option>
                                                    <option value="3">Physical Damage</option>
                                                    <option value="4">General Liability</option>
                                                    <option value="5">Excess Liability</option>
                                                    <option value="6">Other</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Notes</label>
                                            <div class="col-md-9">
                                                <textarea class="form-control" rows="3"  data-model="notes"></textarea>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Bind Date</label>
                                            <div class="col-md-9">
                                                <input type="date" class="form-control" id="txtPolicyBindDate" placeholder="mm/dd/yyy" data-model="bindDate">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Effective</label>
                                            <div class="col-md-9">
                                                <input type="date" class="form-control" id="txtPolicyEffectiveDate" placeholder="mm/dd/yyy" data-model="effective" required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Expiration</label>
                                            <div class="col-md-9">
                                                <input type="date" class="form-control" id="txtPolicyExpirationDate" placeholder="mm/dd/yyy" data-model="expiration" required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Premium Finance</label>
                                            <div class="col-md-9">
                                                <select class="form-control" id="slcPolicyPremiumFinancers" data-model="bankId">
                                                    <option></option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Account Number</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" placeholder="" value="" data-model="accountNo">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-body" style="padding-bottom:0;padding-top:0;">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label"><strong>Basic Rates</strong></label>
                                            <div class="col-md-9 coverage-rates gross-rate" style="padding-left:63px;padding-top:8px;" data-coverage-types="1,2,3">
                                                <label class="mt-checkbox mt-checkbox-outline">
                                                    <input type="checkbox" id="chkGrossReceipt"> Gross Receipt
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="1" data-coverage-gross="">
                                            <label class="col-md-4 control-label">AL Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="basePerUnit">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('ALRATE');" id="txtPolicyALRate">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group hide coverage-premium">
                                            <label class="col-md-4 control-label" id="lblPremiumLabel">Premium</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" data-model="premium" data-type="decimal">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="1">
                                            <label class="col-md-4 control-label">AL Trailer Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" id="" onkeypress="return false;" placeholder="" data-model="trailerRate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('ALTRAILERRATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="2">
                                            <label class="col-md-4 control-label">MTC Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="basePerUnit">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('MTCRATE');" id="txtPolicyMTCRate">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="2">
                                            <label class="col-md-4 control-label">MTC Trailer Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" data-model="trailerRate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('MTCTRAILERRATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="3">
                                            <label class="col-md-4 control-label">PD Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="pdrate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('PDRATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="3">
                                            <label class="col-md-4 control-label">PD Trailer Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="trailerRate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('PDTRAILERRATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="3">
                                            <label class="col-md-4 control-label">NO Trailer Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="pdNonOwnedTrailerRate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('NOTRAILERRATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates" data-coverage-types="3">
                                            <label class="col-md-4 control-label">Interchange Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" placeholder="" onkeypress="return false;" data-model="trailerInterchangeRate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('INTERCHANGERATE');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates gross-rate" data-coverage-types="1,2">
                                            <label class="col-md-4 control-label">Non-Taxable Fees</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" id="" placeholder="" onkeypress="return false;" data-model="nonTaxedRateUnit">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('NTF');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                       
                                        <div class="form-group coverage-rates gross-rate" data-coverage-types="4,5,6">
                                            <label class="col-md-4 control-label">Broker Fees</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" id="" placeholder="" data-model="brokerFees">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates gross-rate" data-coverage-types="1,2">
                                            <label class="col-md-4 control-label">BF Per Unit</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" id="" placeholder="" onkeypress="return false;" data-model="bfrate">
                                                    <span class="input-group-addon" style="cursor:pointer;" onclick="javascript: PolicyComponent.openRateCalculator('BFPERUNIT');">
                                                        <i class="fa fa-calculator"></i>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group coverage-rates gross-rate" data-coverage-types="1,2,3,4,5,6">
                                            <label class="col-md-4 control-label">Surplus Tax Rate</label>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" data-type="decimal" class="form-control" id="" placeholder="" data-model="surplusTaxRate">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label"><strong>Commissions</strong></label>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Commission Rate</label>
                                            <div class="col-md-8">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="commRate">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Unearned Commissions</label>
                                            <div class="col-md-8">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="commission" required>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label"><strong>Taxes and Fees</strong></label>
                                            <div class="col-md-9" style="padding-left:63px;padding-top:8px;">
                                                <label class="mt-checkbox mt-checkbox-outline">
                                                    <input type="checkbox" data-model="directBill"> Direct Bill?
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Surcharge</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="surcharge" >
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Surplus Tax</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="surplusTax">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Policy Fees</label>
                                            <div class="col-md-7">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="policyFees">
                                            </div>
                                            <div class="col-md-2" style="padding-top:8px;padding-left:0;">
                                                <label class="mt-checkbox mt-checkbox-outline" style="margin-bottom:0;" data-type="decimal">
                                                    <input type="checkbox" id="chkTaxedPolicyFees"> Taxed?
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">MGA Fees</label>
                                            <div class="col-md-7">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="mgafees">
                                            </div>
                                            <div class="col-md-2" style="padding-top:8px;padding-left:0;">
                                                <label class="mt-checkbox mt-checkbox-outline" style="margin-bottom:0;">
                                                    <input type="checkbox" id="chkTaxedMgaFees"> Taxed?
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Other Fees</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" placeholder="" value="" data-type="decimal" data-model="otherFees">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label"><strong>TOTAL</strong></label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" id="txtTaxesTotal" data-model="totalPremium" placeholder="" value="" onkeypress="return false;">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn dark btn-outline" id="btnClosePolicyForm">Close</button>
                    <button type="submit" class="btn btn-info"><i class="fa fa-save"></i>&nbsp;Save</button>
                </div>
            </form>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>